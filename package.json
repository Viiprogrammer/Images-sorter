        }
      ]
    },
    "win": {
      "target": "squirrel"
    }
  },
  "repository": "",
  "keywords": [],
  "author": "Maxim Telegin",
  "license": "MIT",
  "devDependencies": {
    "bootstrap": "^4.1.3",
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1",
    "fs-extra": "^9.0.1",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.4",
    "electron-builder-squirrel-windows": "^22.9.1"
  }
}
